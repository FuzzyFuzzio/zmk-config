// Minimal keymap for keyboard_dongle so build doesn't warn about missing keymap.
// Include key and behavior bindings definitions.

#include <dt-bindings/zmk/keys.h>
#include <behaviors.dtsi>

/ {
    keymap {
        compatible = "zmk,keymap";
        default_layer {
            /* Single dummy key triggers bootloader */
            bindings = <&bootloader>;
        };
    };
};
