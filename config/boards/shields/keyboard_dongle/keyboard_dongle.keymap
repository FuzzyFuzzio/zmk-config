// Minimal keymap for keyboard_dongle so build doesn't warn about missing keymap.
// Single layer with two utility behaviors (reset & bluetooth advertise) if available.

/ {
    keymap {
        compatible = "zmk,keymap";
        default_layer {
            /* Single dummy key for 1x1 matrix; uses standard keypress behavior */
            bindings = <&kp A>;
        };
    };
};
